[{"Name": "3.805 Exemption.", "Content": "The Secretary of Defense may exempt, on a case-by-case basis, a covered Federal action from the prohibitions of this subpart whenever the Secretary determines, in writing, that such an exemption is in the national interest. The Secretary shall transmit a copy of the exemption to Congress immediately after making the determination.", "Notes":"", "Uid":"7f0f4ff6-5269-4f8c-af97-ecce288f244d"}]
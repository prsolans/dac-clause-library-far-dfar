[{"Name": "3.503-1 Policy.", "Content": "10 U.S.C.2402 and 41 U.S.C. 4704 require that subcontractors not be unreasonably precluded from making direct sales to the Government of any supplies or services made or furnished under a contract. However, this does not preclude contractors from asserting rights that are otherwise authorized by law or regulation.", "Notes":"", "Uid":"dcc931f2-82c1-42eb-950f-330adb158add"}]
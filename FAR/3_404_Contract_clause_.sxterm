[{"Name": "3.404 Contract clause.", "Content": "The contracting officer shall insert the clause at 52.203-5, Covenant Against Contingent Fees, in all solicitations and contracts exceeding the simplified acquisition threshold, other than those for commercial items (see parts 2 and 12).", "Notes":"", "Uid":"17bf808e-3be0-414f-ae4c-e7c70145e8d6"}]
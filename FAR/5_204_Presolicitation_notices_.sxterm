[{"Name": "5.204 Presolicitation notices.", "Content": "Contracting officers must provide access to presolicitation notices through the GPE (see 15.201 and 36.213-2). The contracting officer must synopsize a proposed contract action before issuing any resulting solicitation (see 5.201 and 5.203).", "Notes":"", "Uid":"44513ea5-8096-4214-9fbf-d15fc2dffac3"}]
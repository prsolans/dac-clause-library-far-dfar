[{"Name": "1.603-4 Termination.", "Content": "Termination of a contracting officer appointment will be by letter, unless the Certificate of Appointment contains other provisions for automatic termination. Terminations may be for reasons such as reassignment, termination of employment, or unsatisfactory performance. No termination shall operate retroactively.", "Notes":"", "Uid":"58143556-6b17-4ca3-a807-5bc8dcf6bb04"}]
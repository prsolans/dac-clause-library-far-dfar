[{"Name": "1.400 Scope of subpart.", "Content": "This subpart prescribes the policies and procedures for authorizing deviations from the FAR. Exceptions pertaining to the use of forms prescribed by the FAR are covered in part 53 rather than in this subpart.", "Notes":"", "Uid":"56255417-aa24-44d5-834d-6728b3d20162"}]
[{"Name": "9.108-4 Waiver.", "Content": "Any agency head may waive the prohibition in subsection 9.108-2 and the requirement of subsection 9.108-3 for a specific contract if the agency head determines in writing that the waiver is required in the interest of national security, documents the determination, and reports it to the Congress.", "Notes":"", "Uid":"d7efcdc1-cf2c-43e5-9c0a-78c00af84570"}]
[{"Name": "11.604 Solicitation provision and contract clause.", "Content": "(a) Contracting officers shall insert the provision at 52.211-14, Notice of Priority Rating for National Defense, Emergency Preparedness, and Energy Program Use, in solicitations when the contract to be awarded will be a rated order. (b) Contracting officers shall insert the clause at 52.211-15, Defense Priority and Allocation Requirements, in contracts that are rated orders.", "Notes":"", "Uid":"709f4fcc-36d9-4558-bbc0-883e81cc3513"}]
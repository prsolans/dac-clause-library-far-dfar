[{"Name": "8.606 Evaluating FPI performance.", "Content": "Agencies shall evaluate FPI contract performance in accordance with subpart 42.15. Performance evaluations do not negate the requirements of 8.602 and 8.604, but they may be used to support a waiver request in accordance with 8.604.", "Notes":"", "Uid":"3236c260-9086-4d37-8d12-628c2ed06f90"}]
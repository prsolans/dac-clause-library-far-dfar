[{"Name": "8.405 Ordering procedures for Federal Supply Schedules.", "Content": "Ordering activities shall use the ordering procedures of this section when placing an order or establishing a BPA for supplies or services. The procedures in this section apply to all schedules. For establishing BPAs and for orders under BPAs see 8.405-3.", "Notes":"", "Uid":"f373a3fd-aad0-4d85-87de-96dcba67088c"}]
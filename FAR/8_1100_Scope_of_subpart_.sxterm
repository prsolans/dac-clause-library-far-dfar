[{"Name": "8.1100 Scope of subpart.", "Content": "This subpart covers the procedures for the leasing, from commercial concerns, of motor vehicles that comply with Federal Motor Vehicle Safety Standards and applicable State motor vehicle safety regulations. It does not apply to motor vehicles leased outside the United States and its outlying areas.", "Notes":"", "Uid":"48d61839-e011-4f03-8df9-7cfb372dec24"}]
[{"Name": "52.211-15 Defense Priority and Allocation Requirements.", "Content": "As prescribed in 11.604(b), insert the following clause: Defense Priority and Allocation Requirement (Apr 2008) This is a rated order certified for national defense, emergency preparedness, and energy program use, and the Contractor shall follow all the requirements of the Defense Priorities and Allocations System regulation (15 CFR 700). (End of clause)", "Notes":"", "Uid":"45f36c6e-60c6-445d-a31e-931aef72942b"}]
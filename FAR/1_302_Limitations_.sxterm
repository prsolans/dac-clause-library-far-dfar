[{"Name": "1.302 Limitations.", "Content": "Agency acquisition regulations shall be limited to- (a) Those necessary to implement FAR policies and procedures within the agency; and (b) Additional policies, procedures, solicitation provisions, or contract clauses that supplement the FAR to satisfy the specific needs of the agency.", "Notes":"", "Uid":"b21cbc78-5d1a-410b-9e2e-e83abf68cfa9"}]
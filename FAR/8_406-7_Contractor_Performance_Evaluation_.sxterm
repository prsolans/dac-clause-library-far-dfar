[{"Name": "8.406-7 Contractor Performance Evaluation.", "Content": "Ordering activities must prepare at least annually and at the time the work under the order is completed, an evaluation of contractor performance for each order that exceeds the simplified acquisition threshold in accordance with 42.1502(c).", "Notes":"", "Uid":"5f8866b7-be3e-487b-aa99-883b45108c81"}]
[{"Name": "9.109-5 Solicitation provision.", "Content": "Unless the exception at 9.109-3 applies, the contracting officer shall include the provision at 52.209-13, Violation of Arms Control Treaties or Agreements-Certification, in each solicitation for the acquisition of products or services (including construction) that exceeds the simplified acquisition threshold, other than solicitations for the acquisition of commercial items.", "Notes":"", "Uid":"901df749-c818-4aaf-9d79-c45b61c3b471"}]
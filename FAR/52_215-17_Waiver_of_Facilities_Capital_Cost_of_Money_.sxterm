[{"Name": "52.215-17 Waiver of Facilities Capital Cost of Money.", "Content": "As prescribed in 15.408(i), insert the following clause: Waiver of Facilities Capital Cost of Money (Oct 1997) The Contractor did not include facilities capital cost of money as a proposed cost of this contract. Therefore, it is an unallowable cost under this contract. (End of clause)", "Notes":"", "Uid":"ce16db3b-c0f0-4907-9d45-0186b255296b"}]
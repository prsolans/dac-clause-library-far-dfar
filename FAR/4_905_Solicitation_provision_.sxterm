[{"Name": "4.905 Solicitation provision.", "Content": "The contracting officer shall insert the provision at 52.204-3, Taxpayer Identification, in solicitations that- (a) Do not include the provision at 52.204-7, System for Award Management; and (b) Are not conducted under the procedures of part 12.", "Notes":"", "Uid":"4185850e-cc43-4540-9947-6c519bb60638"}]
[{"Name": "3.202 Contract clause.", "Content": "The contracting officer shall insert the clause at 52.203-3, Gratuities, in solicitations and contracts with a value exceeding the simplified acquisition threshold, except those for personal services and those between military departments or defense agencies and foreign governments that do not obligate any funds appropriated to the Department of Defense.", "Notes":"", "Uid":"f6c8540f-7962-4a29-9d28-551ea2043a76"}]